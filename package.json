{
  "name": "reactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx}'",
    "lint-staged": "lint-staged",
    "preclean": "watchman watch-del-all && rm -rf node_modules && yarn",
    "clean": "rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "remetro": "watchman watch-del-all && yarn start --reset-cache",
    "lint": "esw src --color",
    "lint:watch": "yarn lint --clear --watch"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "change-case-object": "^2.0.0",
    "lint-staged": "^9.1.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
    "react": "16.8.6",
    "react-native": "0.60.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-paper": "^2.16.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-watch": "^5.1.2",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "0.55.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "coverageDirectory": "__coverage__",
    "transform": {},
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-navigation|react-native-fabric|react-native-paper)"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "*.json": [
    "prettier --parser json --write",
    "git add"
  ],
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
